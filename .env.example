# Environment variables for Oahu Transit Hub

# Transit APIs (Required)
THEBUS_API_KEY=your_thebus_api_key_here

# AI Services (Required for trip planning)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Mapping & Geocoding (Required)
NEXT_PUBLIC_MAPBOX_ACCESS_TOKEN=your_mapbox_token_here
GOOGLE_PLACES_API_KEY=your_google_places_api_key_here

# Backend (Already configured in production)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# App Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3001
NODE_ENV=development
